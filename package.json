    "@types/node": "^13.1.7",
    "@types/supertest": "^2.0.8",
    "csv": "^5.3.1",
    "csv-stringify": "^5.3.6",
    "husky": "^4.0.10",
    "jest": "^24.9.0",
    "json-2-csv": "^3.5.9",
    "jwt-decode": "^2.2.0",
    "lint-staged": "^9.5.0",
    "nodemon": "^2.0.2",
    "papaparse": "^5.1.1",
