{
  "name": "image-api",
  "version": "0.0.0",
  "description": "description",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "format": "prettier --write src/**/*.ts migrations/**/*.ts test/**/*.ts *.json test/**/*.json",
    "serve": "npm run format && ts-node -r tsconfig-paths/register src/main.ts",
    "serve:dev": "nodemon",
    "serve:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "serve:prod": "node -r tsconfig-paths/register dist/src/main.js",
    "lint": "./node_modules/tslint/bin/tslint --fix --project .",
    "test": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@babel/preset-env": "^7.3.4",
    "@nestjs/common": "^5.4.0",
    "@nestjs/core": "^5.4.0",
    "@nestjs/typeorm": "^5.3.0",
    "@types/jest": "^23.3.14",
    "@types/redis": "^2.8.10",
    "bcrypt": "^3.0.4",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "db-migrate-sqlite3": "^0.4.0",
    "dotenv": "^6.2.0",
    "eslint": "^5.14.1",
    "fs": "0.0.1-security",
    "http-status-codes": "^1.3.2",
    "import": "0.0.6",
    "jest": "^23.6.0",
    "json2csv": "^4.4.1",
    "jsonwebtoken": "^8.4.0",
    "redis": "^2.8.0",
    "redis-server": "^1.2.2",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.4.0",
    "sqlite3": "^4.0.6",
    "ts-jest": "^23.10.5",
    "tslint-etc": "^1.5.0",
    "typeorm": "^0.2.15",
    "typescript": "^3.0.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.7.3",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.14",
    "@types/node": "^10.7.1",
    "@types/supertest": "^2.0.5",
    "csv": "^5.1.1",
    "csv-stringify": "^5.3.0",
    "husky": "^1.3.1",
    "jest": "^23.5.0",
    "json-2-csv": "^3.5.0",
    "jwt-decode": "^2.2.0",
    "lint-staged": "^8.1.4",
    "nodemon": "^1.18.3",
    "papaparse": "^4.6.3",
    "prettier": "^1.14.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.10.5",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.8.0",
    "tslint": "5.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|ts)": [
      "prettier",
      "./node_modules/tslint/bin/tslint --fix",
      "git add"
    ]
  }
}
